/* 密码 */

.passwrod {
    width: 100%;
    height: 100vh;
    background-color: rgba(0, 0, 0, .5);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 102;
}

.pass {
    width: 70%;
    background-color: #fff;
    border-radius: 10rpx;
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-50%, -50%);
    text-align: center;
    padding: 40rpx 0;
    overflow: hidden;
    min-height: 175rpx;
}

.pass>image {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    width: 40rpx;
}

.tit {
    padding-bottom: 40rpx;
}

.wrod {
    width: 90%;
    margin: 0 auto;
    display: flex;
    justify-content: flex-start;
}

.wrod input {
    width: 80rpx;
    height: 80rpx;
    border: 1rpx solid #BFBFBF;
    border-right: none;
}

.wrod input:last-child {
    border: 1rpx solid #BFBFBF;
}

.input {
    position: relative;
    text-align: center;
}

.input>input {
    position: absolute;
    width: 300%;
    height: 68rpx;
    top: 0;
    left: -300rpx;
    text-align: left;
    z-index: 105;
}