/* pages/card/ms/ms.wxss */
.head {
    margin: 28rpx 55rpx 44rpx 55rpx;
    height: 27rpx;
    width: 640rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.line {
    height: 2rpx;
    background:rgba(132,97,38,1);
    flex-grow: 1;
}
.text{
    font-size:28rpx;
    font-weight:500;
    color:rgba(132,97,38,1);
    margin: 0 15rpx;
}
.cont{
    margin: 14rpx 45rpx 0 45rpx;
    width: 660rpx;
}
.quan{
    position: relative;
    margin-top: 30rpx;
    width: 660rpx;
    height: 230rpx;
}
.quan image{
    width: 660rpx;
    height: 230rpx;
}
.button{
    position: absolute;
    left: 481rpx;
    top: 93rpx;
    width:150rpx;
    height:43rpx;
    background:rgba(255,237,228,1);
    border-radius:22rpx;
    font-size:23rpx;
    font-weight:500;
    color:rgba(234,122,68,1);
    line-height: 43rpx;
    text-align: center;
}

.wrap{
    position: fixed;
    z-index: 10;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    align-items: center;
    justify-content: center;
}
.f-w{
    width: 540rpx;
    height: 500rpx;
    background:rgba(255,255,255,1);
    border-radius:20rpx;
}
.f-1{
    position: relative;
    top: 61rpx;
    width: 540rpx;
    font-size:30rpx;
    font-weight:500;
    color:rgba(0,0,0,1);
    text-align: center;
}
.f-0{
    width: 540rpx;
    height: 60rpx;
}
.f-2{
    margin: 0 auto;
    margin-top: 64rpx;
    width: 300rpx; 
    height: 300rpx;
}
.f-2 image{
   width: 300rpx; 
   height: 300rpx; 
}